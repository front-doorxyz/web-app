{
  "name": "@se-2/nextjs",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "start": "next dev",
    "build": "next build",
    "build:yolo": "export NEXT_PUBLIC_IGNORE_BUILD_ERROR=true && next build",
    "serve": "next start",
    "lint": "next lint",
    "format": "prettier --write . '!(node_module|.next|contracts)/**/*'",
    "check-types": "tsc --noEmit --incremental",
    "vercel": "vercel",
    "vercel:yolo": "vercel --build-env NEXT_PUBLIC_IGNORE_BUILD_ERROR=true",
    "storage-playground": "ts-node ./services/store/playground.ts",
    "aws:profile": "export AWS_PROFILE=default",
    "aws:purge": "yarn aws:profile && aws cloudfront create-invalidation --distribution-id E10U3QXB9T5GTI --paths \"/*\"",
    "aws:release": "yarn aws:profile && aws s3 cp \"$PWD/out\" s3://djinndao --recursive",
    "aws": "yarn aws:profile && yarn build:yolo && yarn aws:release && yarn aws:purge"
  },
